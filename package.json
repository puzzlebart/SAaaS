{
  "name": "saaas",
  "version": "1.0.0",
  "description": "Shitty APIs as a Service",
  "engines": {
    "node": "^8.12.0",
    "npm": "^6.4.1"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "dev": "watch 'npm run build-babel && node ./build/index.js' ./src",
    "start": "node ./build/index.js",
    "watch": "watch 'npm run build-babel' ./src"
  },
  "keywords": [
    "ACDC2019",
    "Simpsons",
    "API"
  ],
  "author": "damsleth",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.2",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "jsdom": "^13.2.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "watch": "^1.0.2",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "nodemon": "^1.18.10"
  }
}